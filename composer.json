{    "name": "dvdmeneses/buscador-cursos",    "description": "Projeto que busca os cursos no site da Alura",    "version": "1.1.0",    "type": "library",    "autoload": {        "files": [            "./functions.php"        ],        "psr-4": {            "Dvdmeneses\\BuscadorDeCurso\\": "src/BuscadorDeCurso/"        }    },    "authors": [        {            "name": "DvdMeneses",            "email": "david.meneses.cc@gmail.com"        }    ],    "require": {        "guzzlehttp/guzzle": "^7.8",        "symfony/dom-crawler": "^7.1",        "symfony/css-selector": "^7.1"    },    "require-dev": {        "phpunit/phpunit": "11",        "squizlabs/php_codesniffer": "*",        "phan/phan": "^5.4"    },    "scripts": {        "test": "phpunit Testes/TestBuscadorDeCursos.php",        "cs": "phpcs --standard=PSR12 src/",        "phan": "phan --allow-polyfill-parser",        "check": [            "@phan",            "@cs",            "@test"        ],        "post-update-cmd": [            "@test"        ]    },    "scripts-descriptions": {        "check": "Roda as verificações do codigo. PHAN , PHPCS E PHPUNIT"    },    "license": "GPL-3.0",    "bin": ["buscar-cursos.php"]}